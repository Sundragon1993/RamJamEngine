<?xml version="1.0"?>

<scene>
	<gameobject>
		<name>valley</name>
		<transform>
			<position x="0" y="0" z="0"/>
			<rotation x="0" y="0" z="0"/>
			<scale    x="0.01" y="0.01" z="0.01"/>
		</transform>
		<mesh>
			<file>valley.mesh</file>
			<material>model.mat</material>
		</mesh>
	</gameobject>
	<gameobject>
		<name>grid</name>
		<transform>
			<position x="0" y="0" z="0"/>
			<rotation x="0" y="0" z="0"/>
			<scale    x="1" y="1" z="1"/>
		</transform>
		<mesh>
			<primitive>grid</primitive>
			<parameters width="100" depth="100" rows="2" columns="2"/>
			<material>grid.mat</material>
		</mesh>
	</gameobject>
	<gameobject>
		<name>cylinder</name>
		<transform>
			<position x="5" y="0" z="5"/>
			<rotation x="0" y="0" z="0"/>
			<scale    x="1" y="1" z="1"/>
		</transform>
		<mesh>
			<primitive>cylinder</primitive>
			<parameters bottomRadius="0.5" topRadius="0.3" height="3" slices="20" stacks="20"/>
			<material>cylinder.mat</material>
		</mesh>
	</gameobject>
	<gameobject>
		<name>box</name>
		<transform>
			<position x="-5" y="0" z="5"/>
			<rotation x="0" y="0" z="0"/>
			<scale    x="1" y="1" z="1"/>
		</transform>
		<mesh>
			<primitive>box</primitive>
			<parameters width="1" height="1" depth="1"/>
			<material>box.mat</material>
		</mesh>
	</gameobject>
	<gameobject>
		<name>geosphere</name>
		<transform>
			<position x="-5" y="0" z="-5"/>
			<rotation x="0" y="0" z="0"/>
			<scale    x="1" y="1" z="1"/>
		</transform>
		<mesh>
			<primitive>geosphere</primitive>
			<parameters radius="0.5" subdiv="3"/>
			<material>sphere.mat</material>
		</mesh>
	</gameobject>
</scene>
